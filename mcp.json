{
  "mcpServers": {
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "figma": {
      "command": "npx",
      "args": [
        "figma-mcp"
      ],
      "env": {
        "FIGMA_API_KEY": "1234"
      }
    }
  }
}
